<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="roundedCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

    <style name="NotsAlertDialog" parent="ThemeOverlay.Material3.MaterialAlertDialog.Centered">
        <item name="android:background">?attr/colorSurface</item>
    </style>

    <style name="Widget.App.ActionMode" parent="Widget.AppCompat.ActionMode">
        <item name="background">?actionModeBackgroundColor</item>
        <item name="titleTextStyle">@style/TitleActionBar</item>
    </style>

    <style name="TitleActionBar" parent="TextAppearance.Material3.ActionBar.Title">
        <item name="android:textColor">?actionModeTextColor</item>
    </style>

    <style name="AppBarTitle" parent="android:TextAppearance.Material.Widget.Toolbar.Title">
        <item name="fontFamily">@font/inter_bold</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>

    <style name="ChipAppearance" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerSize">20dp</item>
        <item name="cornerFamily">rounded</item>
    </style>

</resources>