<resources>
    <style name="Theme.Nots" parent="Theme.Material3.Light.NoActionBar">
        <item name="background">@color/black</item>
        <item name="colorSurface">@color/surface_color</item>
        <item name="colorOnSurface">@color/on_surface_color</item>
        <item name="colorPrimary">#FF5AB0FF</item>
        <item name="colorOnPrimary">#FF212121</item>
        <item name="colorSecondary">#FFCBEFFF</item>
        <item name="colorOnSecondary">#FF212121</item>
        <item name="android:statusBarColor">?attr/colorPrimary</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeBackgroundColor">#FF212121</item>
        <item name="actionModeTextColor">#FFFAFAFA</item>
        <item name="actionModeStyle">@style/Widget.App.ActionMode</item>
        <item name="actionModeCloseDrawable">@drawable/ic_close</item>
        <item name="actionModeCloseDrawableColor">#FFFAFAFA</item>
        <item name="actionBarTheme">@style/ThemeOverlay.AppCompat.ActionBar</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen.IconBackground">
        <item name="windowSplashScreenBackground">@color/surface_color</item>
        <item name="windowSplashScreenIconBackgroundColor">@color/on_surface_color</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_launcher_foreground_black</item>
        <item name="postSplashScreenTheme">@style/Theme.Nots</item>
    </style>
</resources>